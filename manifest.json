{
  "name": "EVENTOS MEGAMU",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extens√£o para ajudar a ficar ligado nos eventos",
  "homepage_url": "http://megamu.net/d70",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
